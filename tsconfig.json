{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "ignoreDeprecations": "6.0",
    "paths": {
      "@/*": ["src/*"],
      "@tabs/*": ["src/components/ui/tabs/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
